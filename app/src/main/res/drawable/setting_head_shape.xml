<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false">
        <shape>
            <gradient
                android:startColor="#9A76FE"
                android:endColor="#6191F9"
                android:angle="180" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>

            <stroke android:color="#9A76FE"></stroke>
            <solid android:color="#9A76FE" />
        </shape>
    </item>
    <item android:state_enabled="true">
        <shape>
            <gradient
                android:startColor="#9A76FE"
                android:endColor="#6191F9"
                android:angle="180" />
        </shape>
    </item>
</selector>
